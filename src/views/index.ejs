    <!-- JavaScript to Toggle Mobile Menu and Dropdowns -->
    <script>
      // Elements
      const menuToggle = document.getElementById("menu-toggle");
      const mobileMenu = document.getElementById("mobile-menu");
      const menuClose = document.getElementById("menu-close");

      // Open Mobile Menu
      menuToggle.addEventListener("click", () => {
        mobileMenu.classList.remove("translate-x-full");
      });

      // Close Mobile Menu
      menuClose.addEventListener("click", () => {
        mobileMenu.classList.add("translate-x-full");
      });

      // Toggle Dropdowns
      function toggleDropdown(id) {
        const dropdown = document.getElementById(id);
        const allDropdowns = document.querySelectorAll('ul[id$="-dropdown"]');

        // Close other dropdowns
        allDropdowns.forEach((el) => {
          if (el.id !== id) el.classList.add("hidden");
        });

        // Toggle selected dropdown
        dropdown.classList.toggle("hidden");
      }

      // Prevent Navbar from Closing on Header Click
      document
        .querySelectorAll("#mobile-menu > ul > li > a")
        .forEach((headerLink) => {
          headerLink.addEventListener("click", (e) => {
            e.stopPropagation(); // Prevent event bubbling
            const dropdown = headerLink.nextElementSibling;
            if (dropdown && dropdown.classList.contains("hidden")) {
              toggleDropdown(dropdown.id);
            }
          });
        });

      // Prevent Navbar from Closing on Submenu Click
      document
        .querySelectorAll("#mobile-menu ul li ul li a")
        .forEach((subLink) => {
          subLink.addEventListener("click", (e) => {
            e.stopPropagation(); // Prevent event bubbling
          });
        });

      // Close Navbar Only on External Navigation
      document
        .querySelectorAll("#mobile-menu ul li ul li a")
        .forEach((subLink) => {
          subLink.addEventListener("click", (e) => {
            if (subLink.href && !subLink.href.includes("#")) {
              mobileMenu.classList.add("translate-x-full");
            }
          });
        });

      // JavaScript for rotating images
      let current = 0;
      const images = document.querySelectorAll(".carousel img");
      setInterval(() => {
        images[current].classList.remove("opacity-100");
        images[current].classList.add("opacity-0");
        current = (current + 1) % images.length;
        images[current].classList.remove("opacity-0");
        images[current].classList.add("opacity-100");
      }, 3000);
    </script>
